{
  "name": "auditpath",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "style": "stylelint \"**/*.css\"",
    "style:fix": "stylelint \"**/*.css\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "preview": "vite preview",
    "db:seed": "tsx scripts/db/seed-db.ts",
    "sync": "tsx scripts/notion-sync/index.ts",
    "update-types": "dotenv -e .env.local -- npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.types.ts",
    "check-all": "npm run type-check && npm run lint && npm run style && npm run format",
    "prepare": "husky"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-virtual": "^3.13.18",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.34.0",
    "katex": "^0.16.27",
    "lucide-react": "^0.562.0",
    "mermaid": "^11.12.2",
    "p-limit": "^7.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-medium-image-zoom": "^5.4.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.15",
    "@tailwindcss/postcss": "^4",
    "@types/canvas-confetti": "^1.9.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "notion-to-md": "^3.1.9",
    "pg": "^8.16.3",
    "prettier": "^3.8.1",
    "sharp": "^0.34.5",
    "stylelint": "^17.3.0",
    "stylelint-config-standard": "^40.0.0",
    "supabase": "^2.76.3",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint:fix"
    ],
    "*.css": [
      "npm run style:fix"
    ],
    "*": [
      "npm run format:fix"
    ]
  }
}
